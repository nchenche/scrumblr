<% if (typeof pageScripts !== 'undefined') { %>
    <% pageScripts.forEach(function(script) { %>
        <script type="module" src="<%= script %>"></script>
    <% }); %>
<% } %>

<!-- Check if the variable object is defined -->
<% if (typeof variable !== 'undefined') { %>
    <script>
        // Define the global variable GLOB_VAR with the value from server-side
        var GLOB_VAR = <%- JSON.stringify(variable) %>;
    </script>
<% } %>